{
  "name": "laji-form-builder",
  "version": "0.0.0",
  "description": "UI for building laji.fi forms",
  "main": "lib/app.js",
  "scripts": {
    "start": "webpack-dev-server --content-base playground playground/app.tsx --inline --port 8082 --host 0.0.0.0",
    "staging": "webpack-dev-server --content-base playground playground/app.tsx --inline --port 4010 --host 0.0.0.0 --public playground.luomus.fi:8080",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint -c .eslintrc.prod.json src playground",
    "build:lib": "rimraf lib && NODE_ENV=production tsc -p tsconfig.lib.json",
    "build:dist": "rimraf dist && NODE_ENV=production webpack --config webpack.config.dist.js && rimraf dist/styles.js",
    "build": "npm run build:lib && npm run build:dist"
  },
  "keywords": [
    "laji-form",
    "laji.fi",
    "FinBIF",
    "luomus",
    "react-jsonschema-form"
  ],
  "packages": {
    "start": "webpack-dev-server --content-base playground playground/app.tsx --inline --port 8084 --host 0.0.0.0"
  },
  "dependencies": {
    "@types/react-bootstrap": "^0.32.25",
    "deepmerge": "^4.2.2",
    "isomorphic-fetch": "^2.2.1",
    "laji-form": "^11.0.8",
    "memoizee": "^0.4.14",
    "parse-prop-types": "git+https://git@github.com/olzraiti/parse-prop-types.git#c8a4540d8d0af0e47187c1d0de155c11be84b9ce",
    "react": "~16.8.6",
    "react-bootstrap": "^0.33.1",
    "react-dom": "~16.8.6",
    "react-jsonschema-form": "git+https://git@github.com/olzraiti/react-jsonschema-form.git#0443d2f7ff0d9e9eb5ead2de7c8e5bfcdc01d634",
    "react-spinner": "^0.2.7"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "0.0.35",
    "@types/memoize-one": "^4.1.1",
    "@types/memoizee": "^0.4.3",
    "@types/node": "^12.7.5",
    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.6",
    "@types/react-spinner": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^6.3.0",
    "css-loader": "^3.2.0",
    "eslint": "^7.12.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^4.2.0",
    "mini-css-extract-plugin": "^1.2.1",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "svg-url-loader": "^3.0.2",
    "typescript": "^4.1.5",
    "url-loader": "^2.1.0",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "resolutions": {
    "leaflet": "1.4.0"
  },
  "author": "Olli Raitio",
  "license": "MIT",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luomus/laji-form-builder.git"
  },
  "bugs": {
    "url": "https://github.com/luomus/laji-form-builder/issues"
  },
  "homepage": "https://github.com/luomus/laji-form-builder#readme"
}
