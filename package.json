{
  "name": "laji-form-builder",
  "version": "0.1.11",
  "description": "UI for building laji.fi forms",
  "main": "lib/app.js",
  "scripts": {
    "start": "npx webpack serve",
    "test": "HOST=127.0.0.1 PORT=8082 protractor protractor.conf.js",
    "lint": "eslint -c .eslintrc.prod.json src playground",
    "build:lib": "rimraf lib && NODE_ENV=production tsc -p tsconfig.lib.json",
    "build:dist": "rimraf dist && NODE_ENV=production webpack --config webpack.config.dist.js && rimraf dist/styles.js",
    "build": "npm run build:lib && npm run build:dist",
    "preversion": "npm run lint && npm test",
    "postversion": "npm run build && npm publish && git push"
  },
  "keywords": [
    "laji-form",
    "laji.fi",
    "FinBIF",
    "luomus",
    "react-jsonschema-form"
  ],
  "dependencies": {
    "@types/isomorphic-fetch": "0.0.35",
    "@types/memoize-one": "^4.1.1",
    "@types/memoizee": "^0.4.3",
    "@types/node": "^12.7.5",
    "@types/react": "^16.14.14",
    "@types/react-bootstrap": "^0.32.25",
    "@types/react-dom": "^17.0.9",
    "@types/react-spinner": "^0.2.0",
    "deepmerge": "^4.2.2",
    "isomorphic-fetch": "^3.0.0",
    "json-schema": "^0.3.0",
    "laji-form": "^12.0.24",
    "memoizee": "^0.4.14",
    "parse-prop-types": "git+https://git@github.com/olzraiti/parse-prop-types.git#c8a4540d8d0af0e47187c1d0de155c11be84b9ce",
    "react": "^17.0.2",
    "react-bootstrap": "^0.33.1",
    "react-dom": "^17.0.2",
    "react-jsonschema-form": "git+https://git@github.com/olzraiti/react-jsonschema-form.git#0443d2f7ff0d9e9eb5ead2de7c8e5bfcdc01d634",
    "react-spinner": "^0.2.7"
  },
  "devDependencies": {
    "@types/jasmine": "^3.9.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-protractor": "^2.1.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "ignore-loader": "^0.1.2",
    "mini-css-extract-plugin": "^2.2.0",
    "node-sass": "^6.0.1",
    "protractor": "^7.0.0",
    "protractor-console-plugin": "^0.1.1",
    "sass-loader": "^12.1.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1"
  },
  "author": "Olli Raitio",
  "license": "MIT",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luomus/laji-form-builder.git"
  },
  "bugs": {
    "url": "https://github.com/luomus/laji-form-builder/issues"
  },
  "homepage": "https://github.com/luomus/laji-form-builder#readme"
}
